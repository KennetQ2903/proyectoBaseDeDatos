<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.proyecto.kennet.eduardo.raul.francisco.proyectobasededatos1.Controllers.LoginController">
   <children>
      <VBox alignment="CENTER" layoutX="150.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="402.0" prefWidth="288.0" spacing="15.0">
         <children>
            <Label text="Inicio de Sesión">
               <font>
                  <Font name="Marlett" size="24.0" />
               </font>
               <padding>
                  <Insets bottom="20.0" />
               </padding>
            </Label>
            <TextField fx:id="username" promptText="Nombre de usuario" />
            <PasswordField fx:id="password" promptText="Contraseña" />
            <Button fx:id="loginButton" mnemonicParsing="false" onAction="#handleSubmit" prefHeight="25.0" prefWidth="288.0" text="Iniciar sesion" />
         </children>
         <padding>
            <Insets bottom="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
